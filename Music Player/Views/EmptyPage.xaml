<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:usercontrols="clr-namespace:Music_Player.Views.UserControls"
             x:Class="Music_Player.Views.EmptyPage"
             BackgroundColor="{StaticResource Back}">
    <ContentPage.Content>
        <Grid x:Name="Grid">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <usercontrols:BottomTrackView Grid.Row="1"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>